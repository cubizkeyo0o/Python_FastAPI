

def dto_to_orm(user_orm: UserORM) -> User:
    """
    Chuyển từ SQLAlchemy ORM model -> Domain entity
    """
    return User(
        id=user_orm.id,
        email=user_orm.email,
        full_name=user_orm.full_name,
    )

def orm_to_dto(user_orm: UserORM) -> User:
    """
    Chuyển từ SQLAlchemy ORM model -> Domain entity
    """
    return User(
        id=user_orm.id,
        email=user_orm.email,
        full_name=user_orm.full_name,
    )